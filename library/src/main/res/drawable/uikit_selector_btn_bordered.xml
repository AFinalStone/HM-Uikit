<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <!-- 填充颜色 -->
            <solid android:color="#FFF9F9F9"></solid>
            <!-- 矩形的圆角半径 -->
            <corners android:radius="@dimen/uikit_shape_common_btn_corners_radius" />
            <stroke android:width="1dip" android:color="@color/uikit_text_main_content"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <!-- 填充颜色 -->
            <solid android:color="#FFFFFFFF"></solid>
            <!-- 矩形的圆角半径 -->
            <corners android:radius="@dimen/uikit_shape_common_btn_corners_radius" />
            <stroke android:width="1dip" android:color="@color/uikit_text_main_content"/>
        </shape>
    </item>
</selector>
