<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hm.iou.uikit.demo">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/UikitAppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TabActivity" />
        <activity android:name=".InputCodeActivity" />
        <activity android:name=".PasswordHideShowActivity" />
        <activity android:name=".tabview.BottomTabViewActivity" />
        <activity
            android:name=".tabview.IndexAddActivity"
            android:theme="@style/UikitTranslucentThemeActivity" />
        <activity android:name=".rich.RichEditActivity" />
        <activity android:name=".rich.RichTextActivity" />
    </application>

</manifest>